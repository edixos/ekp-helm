{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "panels": [
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": {
                "0": { "color": "red", "text": "DOWN" },
                "1": { "color": "green", "text": "UP" }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "textMode": "value"
      },
      "targets": [{ "expr": "up{job=\"keto-metrics\"}", "refId": "A" }],
      "title": "Keto Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 4, "w": 3, "x": 3, "y": 0 },
      "id": 2,
      "options": { "colorMode": "value", "graphMode": "area" },
      "targets": [
        { "expr": "go_goroutines{job=\"keto-metrics\"}", "refId": "A" }
      ],
      "title": "Goroutines",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 4, "w": 3, "x": 6, "y": 0 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area" },
      "targets": [
        {
          "expr": "process_resident_memory_bytes{job=\"keto-metrics\"}",
          "refId": "A"
        }
      ],
      "title": "Memory",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "reqps" } },
      "gridPos": { "h": 4, "w": 3, "x": 9, "y": 0 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"keto-metrics\"}[5m]))",
          "refId": "A"
        }
      ],
      "title": "HTTP Req/s",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "reqps" } },
      "gridPos": { "h": 4, "w": 3, "x": 12, "y": 0 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "area" },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_started_total{job=\"keto-metrics\"}[5m]))",
          "refId": "A"
        }
      ],
      "title": "gRPC Req/s",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "description": "Keto version",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "blue", "value": null }]
          }
        }
      },
      "gridPos": { "h": 4, "w": 3, "x": 15, "y": 0 },
      "id": 6,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "textMode": "name"
      },
      "targets": [
        {
          "expr": "http_requests_total{job=\"keto-metrics\"}",
          "format": "table",
          "refId": "A"
        }
      ],
      "title": "Version",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true,
              "__name__": true,
              "app": true,
              "buildTime": true,
              "code": true,
              "container": true,
              "endpoint": true,
              "hash": true,
              "instance": true,
              "job": true,
              "method": true,
              "namespace": true,
              "pod": true,
              "service": true
            },
            "indexByName": {},
            "renameByName": {}
          }
        }
      ],
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "reqps" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "id": 7,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"keto-metrics\"}[5m])) by (endpoint, method)",
          "legendFormat": "{{method}} {{endpoint}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Request Rate by Endpoint",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "stacking": { "mode": "normal" } }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "id": 8,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(http_requests_statuses_total{job=\"keto-metrics\"}[5m])) by (status_bucket)",
          "legendFormat": "{{status_bucket}}",
          "refId": "A"
        }
      ],
      "title": "HTTP Status Codes",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "reqps" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "id": 9,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_started_total{job=\"keto-metrics\"}[5m])) by (grpc_service, grpc_method)",
          "legendFormat": "{{grpc_service}}/{{grpc_method}}",
          "refId": "A"
        }
      ],
      "title": "gRPC Request Rate by Method",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "stacking": { "mode": "normal" } }
        }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "id": 10,
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum(rate(grpc_server_handled_total{job=\"keto-metrics\",grpc_code!=\"OK\"}[5m])) by (grpc_code)",
          "legendFormat": "{{grpc_code}}",
          "refId": "A"
        }
      ],
      "title": "gRPC Error Codes",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "id": 11,
      "options": {
        "legend": {
          "calcs": ["mean", "max", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_requests_duration_seconds_bucket{job=\"keto-metrics\"}[5m])) by (le, endpoint))",
          "legendFormat": "p95 - {{endpoint}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_requests_duration_seconds_bucket{job=\"keto-metrics\"}[5m])) by (le, endpoint))",
          "legendFormat": "p50 - {{endpoint}}",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_requests_duration_seconds_bucket{job=\"keto-metrics\"}[5m])) by (le, endpoint))",
          "legendFormat": "p99 - {{endpoint}}",
          "refId": "C"
        }
      ],
      "title": "HTTP Request Latency (p50, p95, p99)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "id": 12,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(grpc_server_handling_seconds_bucket{job=\"keto-metrics\"}[5m])) by (le, grpc_method))",
          "legendFormat": "p95 - {{grpc_method}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(grpc_server_handling_seconds_bucket{job=\"keto-metrics\"}[5m])) by (le, grpc_method))",
          "legendFormat": "p50 - {{grpc_method}}",
          "refId": "B"
        }
      ],
      "title": "gRPC Request Latency",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "bytes" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
      "id": 13,
      "options": {
        "legend": {
          "calcs": ["mean", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "go_memstats_heap_alloc_bytes{job=\"keto-metrics\"}",
          "legendFormat": "Heap Allocated",
          "refId": "A"
        },
        {
          "expr": "go_memstats_heap_inuse_bytes{job=\"keto-metrics\"}",
          "legendFormat": "Heap In Use",
          "refId": "B"
        },
        {
          "expr": "go_memstats_heap_idle_bytes{job=\"keto-metrics\"}",
          "legendFormat": "Heap Idle",
          "refId": "C"
        },
        {
          "expr": "go_memstats_stack_inuse_bytes{job=\"keto-metrics\"}",
          "legendFormat": "Stack In Use",
          "refId": "D"
        }
      ],
      "title": "Memory Breakdown",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 28 },
      "id": 14,
      "options": {
        "legend": {
          "calcs": ["mean", "last"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "go_goroutines{job=\"keto-metrics\"}",
          "legendFormat": "Goroutines",
          "refId": "A"
        },
        {
          "expr": "go_threads{job=\"keto-metrics\"}",
          "legendFormat": "OS Threads",
          "refId": "B"
        }
      ],
      "title": "Goroutines & Threads",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "s" } },
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 28 },
      "id": 15,
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "go_gc_duration_seconds{job=\"keto-metrics\", quantile=\"0.5\"}",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "go_gc_duration_seconds{job=\"keto-metrics\", quantile=\"0.75\"}",
          "legendFormat": "p75",
          "refId": "B"
        },
        {
          "expr": "go_gc_duration_seconds{job=\"keto-metrics\", quantile=\"1\"}",
          "legendFormat": "Max",
          "refId": "C"
        }
      ],
      "title": "GC Duration",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "binBps" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 36 },
      "id": 16,
      "options": {
        "legend": {
          "calcs": ["mean"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(process_network_receive_bytes_total{job=\"keto-metrics\"}[5m])",
          "legendFormat": "Network In",
          "refId": "A"
        },
        {
          "expr": "rate(process_network_transmit_bytes_total{job=\"keto-metrics\"}[5m])",
          "legendFormat": "Network Out",
          "refId": "B"
        }
      ],
      "title": "Network I/O",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 36 },
      "id": 17,
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "process_open_fds{job=\"keto-metrics\"}",
          "legendFormat": "Open FDs",
          "refId": "A"
        },
        {
          "expr": "process_max_fds{job=\"keto-metrics\"}",
          "legendFormat": "Max FDs",
          "refId": "B"
        }
      ],
      "title": "File Descriptors",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "ops" } },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 44 },
      "id": 18,
      "options": {
        "legend": {
          "calcs": ["mean"],
          "displayMode": "table",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "expr": "rate(go_memstats_alloc_bytes_total{job=\"keto-metrics\"}[5m])",
          "legendFormat": "Allocations Rate",
          "refId": "A"
        },
        {
          "expr": "rate(go_memstats_mallocs_total{job=\"keto-metrics\"}[5m])",
          "legendFormat": "Mallocs/s",
          "refId": "B"
        },
        {
          "expr": "rate(go_memstats_frees_total{job=\"keto-metrics\"}[5m])",
          "legendFormat": "Frees/s",
          "refId": "C"
        }
      ],
      "title": "Memory Allocations",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 44 },
      "id": 19,
      "options": {
        "legend": {
          "calcs": ["last"],
          "displayMode": "table",
          "placement": "bottom",
          "sortBy": "Last",
          "sortDesc": true
        }
      },
      "targets": [
        {
          "expr": "topk(10, sum(rate(grpc_server_handled_total{job=\"keto-metrics\"}[5m])) by (grpc_service, grpc_method, grpc_code))",
          "legendFormat": "{{grpc_service}}/{{grpc_method}} [{{grpc_code}}]",
          "refId": "A"
        }
      ],
      "title": "Top gRPC Methods by Request Rate",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
  "schemaVersion": 38,
  "tags": ["keto", "ory", "authorization", "grpc"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "",
  "title": "Keto Monitoring Dashboard",
  "uid": "keto-monitoring",
  "version": 0
}
